---
const { title } = Astro.props;
interface Props {
  title: string;
}
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>TechX | {title}</title>
  </head>
  <body>
    <main class="container mx-auto">
      <slot />
    </main>
  </body>
</html>
